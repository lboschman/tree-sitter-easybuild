{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "easybuild",
      "camelcase": "Easybuild",
      "title": "Easybuild",
      "scope": "source.easybuild",
      "file-types": [
        "eb",
        "easybuild"
      ],
      "injection-regex": "^easybuild$",
      "class-name": "TreeSitterEasybuild"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPL-2.0-or-later",
    "description": "Parser for easybuild config files",
    "authors": [
      {
        "name": "Leon Boschman",
        "email": "leon.boschman@chalmers.se"
      }
    ],
    "links": {
      "repository": "https://github.com/lboschman/tree-sitter-easybuild"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}