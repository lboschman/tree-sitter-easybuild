{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "easybuild",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_definition"
        },
        {
          "type": "SYMBOL",
          "name": "version_definition"
        }
      ]
    },
    "string_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "value_string"
        }
      ]
    },
    "version_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "version ="
        },
        {
          "type": "SYMBOL",
          "name": "version_string"
        }
      ]
    },
    "array_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "string_array"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "value_string": {
      "type": "PATTERN",
      "value": "\\'.+\\'"
    },
    "version_string": {
      "type": "PATTERN",
      "value": "\\'\\d+(\\.\\d+)*\\'"
    },
    "string_array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "value_string"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "value_string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}